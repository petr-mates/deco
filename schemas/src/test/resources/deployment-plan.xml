<deployment-plan xmlns="http://javaee.deco.cz/deployment-plan" version="1.0">
    <application-name>string</application-name>
    <variable-definitions>
        <variable>
            <description>description</description>
            <name>variable_name</name>
            <value>variable_value</value>
        </variable>
    </variable-definitions>
    <descriptor-override>
        <path></path>
    </descriptor-override>
    <descriptor-override>
        <path>**/*.java</path>
        <module-descriptor>
            <uri>/**/*.java</uri>
            <insert type="insertAfter">
                <xpath></xpath>
                <value>${value}</value>
            </insert>
            <replace type="content">
                <xpath></xpath>
                <value>
                    <xmlValue/>
                </value>
            </replace>
        </module-descriptor>
    </descriptor-override>
</deployment-plan>